#max_allowed_packet     = 1G
#thread_stack           = 192K
#thread_cache_size      = 8
#myisam_recover_options = BACKUP
#max_connections        = 100
#table_cache            = 64

[mysqld_safe]

nice = 0
skip_log_error
syslog
